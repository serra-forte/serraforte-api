  "main": "index.js",
  "engines": {
    "node": ">=18"
  },
  "scripts": {
    "start:dev": "tsx watch ./src/server.ts",
    "build": "tsup src --out-dir build --ignore-watch views ",
    "postbuild": "cp -r ./views ./build  && cp -r ./src/tmp ./build && cp -r ./src/public ./build",
    "start:prod": "node ./build/server.js",
    "migrate": "prisma migrate dev",
    "test:create-prisma-environment": "npm link ./prisma/vitest-environment-prisma",
    "test:install-prisma-environment": "npm link vitest-environment-prisma",
    "test:unit": "vitest run --dir src/usecases",
    "pretest:e2e": "run-s test:create-prisma-environment test:install-prisma-environment",
    "test:e2e": "vitest run --dir src/http --allowOnly",
    "test:e2e:watch": "vitest --dir src/http --allowOnly",
    "test:unit:watch": "vitest --watch",
    "test:coverage": "vitest --coverage",
    "test:ui": "vitest --ui",
    "test:only": "vitest related --watch",
    "send:email": "tsx ./src/scripts/send-email-fake.ts",
    "seed:user": "node ./build/repositories/seeders/seed-super.js",
    "prisma:studio": "npx prisma studio"
  },
